{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/index.ts": {
            "includeFiles": "server/cms_data.json"
        }
    }
}